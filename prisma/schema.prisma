model bank_accounts {
  id         Int      @id @default(autoincrement())
  OwnerID    Int
  AccNumber  BigInt   @unique(map: "account_number")
  AccName    String   @db.VarChar(50)
  Balance    BigInt?  @default(0)
  created_at DateTime @default(now()) @db.Timestamp(0)
}

model bank_transactions {
  id              Int                    @id @default(autoincrement())
  AccountID       Int
  Type            bank_transactions_Type
  Amount          BigInt
  TargetAccountID Int?                   @default(-1)
  Description     String                 @default("None") @db.VarChar(128)
  MadeDescription String                 @default("None") @db.VarChar(128)
  created_at      DateTime               @default(now()) @db.Timestamp(0)
}

model bill {
  bid     Int    @id
  type    Int    @default(-1)
