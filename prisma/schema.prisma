generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model accounts {
  ID              Int     @id @default(autoincrement())
  email           String  @default("None") @db.VarChar(40)
  Username        String? @db.VarChar(24)
  BindCharacterID Int?    @default(-1)
  Password        String? @db.VarChar(129)
  Password_Bcrypt String? @db.VarChar(129)
  salt            String  @db.VarChar(16)
  verifycode      Int
  DiscordID       BigInt
  regucp          Int
  RegisterDate    String? @db.VarChar(36)
  LoginDate       String? @db.VarChar(36)
  IP              String? @default("n/a") @db.VarChar(16)
  DonatePoint     Int     @default(0)
}

